#Задача 4#

with open('Task5.txt', 'w') as t:
    b = ('aaaaannnnrfgbyujjjjj')
    t.write(b)
    
d = open('file5_1.txt', 'w')   

from collections import Counter
s = Counter(b)
d.write(str(s))
d.close()

f = open('file5_1.txt', 'r')

from collections import Counter
s = Counter(b)
z = []
for k, value in s.items():
    z.append(k * value)
q = ''.join(z)

with open('Task5_2.txt', 'w') as d:
    d.write(str(q))
    
f.close()    


#Задача №5#

from ast import Num
from cmath import sqrt
import math

Ax = float(input('Введите координаты точки Аx:')) 
Ay = float(input('Введите координаты точки Ay:')) 
Bx = float(input('Введите координаты точки Bx:')) 
By = float(input('Введите координаты точки By:'))

sqrt = math.sqrt(((Bx-Ax)**2)+((By-Ay)**2))

print(('Расстояние между точками равно:'), round(sqrt, 3))
